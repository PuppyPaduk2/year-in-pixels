mixin list(items, templateItem)
   if items && items.length
      each item, index in items
         .item(class=(classNameItem || ''))&attributes(item.attrs || {})
            //- Вставим сразу переданный контент, если так есть
            !{item.content || ''}

            //- Если передали шаблон итема
            - var tItem = item.template !== undefined ? item.template : templateItem;
            if tItem
               !{tItem(item)}